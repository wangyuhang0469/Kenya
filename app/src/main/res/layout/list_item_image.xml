<?xml version="1.0" encoding="utf-8"?>
<com.example.administrator.kenya.model.image_selector.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content" android:layout_height="wrap_content">

    <com.example.administrator.kenya.model.image_selector.SquaredImageView
        android:id="@+id/image"
        android:scaleType="centerInside"
        android:src="@drawable/default_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <View
        android:visibility="gone"
        android:id="@+id/mask"
        android:background="#60000000"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageView
        android:id="@+id/checkmark"
        android:layout_gravity="top|right"
        android:layout_marginTop="6dp"
        android:layout_marginRight="6dp"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:src="@drawable/btn_unselected"/>

</com.example.administrator.kenya.model.image_selector.SquareFrameLayout>